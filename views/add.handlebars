<body>
	<div class="container">
		<button><a href="/">Back</a></button>
		{{#each newFriend}}
		<div class="friend">
			<a class="thumbnail">
				<img class="img-responsive img-rounded" src="{{imageURL}}">
			<a class="friendName" href="#"><h4>{{name}}</h4></a>
			<p>{{description}}</p>
			</a>
		</div>
		{{/each}}
	</div>
</body>
